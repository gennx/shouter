<h1>New user</h1>

<%= form_for @user do |f| %>
	<div class="field">
	<%= f.label :email %>
	<%= f.text_field :email %>
	</div>
	<div class="field">
	<%= f.label :username %>
	<%= f.text_field :username %>
	</div>
	<div class="field">
	<%= f.label :profile_image %>
	<%= f.file_field :profile_image %>
	</div>
	
	<%= f.submit %>

<% end %>

<%= render 'form' %>

<%= link_to 'Back', users_path %>
